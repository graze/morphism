{
    "name": "graze/morphism",
    "type": "library",
    "description": "MySQL Database Migration",
    "keywords": [
        "mysql",
        "database",
        "migration",
        "migrate"
    ],
    "homepage": "https://github.com/graze/morphism",
    "license": "MIT",
    "authors": [
        {
            "name": "Graze tech team",
            "homepage": "https://github.com/graze/morphism/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/yaml": "~2.6",
        "doctrine/dbal": "~2.5",
        "symfony/console": "^2.6",
        "symfony/event-dispatcher": "^2.6",
        "illuminate/filesystem": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "symfony/var-dumper": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Graze\\Morphism\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\Morphism\\Test\\" : "test/"
        }
    },
    "bin": [
        "./bin/morphism-diff",
        "./bin/morphism-dump",
        "./bin/morphism-extract",
        "./bin/morphism-lint"
    ]
}
