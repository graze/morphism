{
    "name": "graze/morphism",
    "type": "library",
    "description": "MySQL Database Migration",
    "keywords": [
        "mysql",
        "database",
        "migration",
        "migrate"
    ],
    "homepage": "https://github.com/graze/morphism",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "authors": [
        {
            "name": "Burhan Ali",
            "email": "burhan@graze.com",
            "role": "Developer"
        },
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": "^5.6 | ^7.0",

        "doctrine/dbal": "^2.5",
        "symfony/console": "^2.8 | ^3 | ^4",
        "symfony/yaml": "^2.6 | ^3 | ^4"
    },
    "require-dev": {
        "graze/standards": "^2.0",
        "phpunit/phpunit": "^5.7.21 | ^6 | ^7",
        "squizlabs/php_codesniffer": "^3.3.1",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Graze\\Morphism\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\Morphism\\Test\\" : "tests/"
        }
    },
    "bin": [
        "./bin/morphism"
    ]
}
