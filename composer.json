{
    "name": "graze/morphism",
    "type": "library",
    "description": "MySQL Database Migration",
    "keywords": [
        "mysql",
        "database",
        "migration",
        "migrate"
    ],
    "homepage": "https://github.com/graze/morphism",
    "license": "MIT",
    "config": {
        "platform": {
            "ext-simplexml": "1.0",
            "ext-dom": "1.0",
            "ext-tokenizer": "1.0",
            "ext-xmlwriter": "1.0",
            "php": "5.5"
        }
    },
    "authors": [
        {
            "name": "Burhan Ali",
            "email": "burhan@graze.com",
            "role": "Developer"
        },
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/yaml": "~2.6",
        "doctrine/dbal": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "squizlabs/php_codesniffer": "^2.5",
        "graze/standards": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Graze\\Morphism\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\Morphism\\Test\\" : "tests/"
        }
    },
    "bin": [
        "./bin/morphism-diff",
        "./bin/morphism-dump",
        "./bin/morphism-extract",
        "./bin/morphism-lint"
    ]
}
